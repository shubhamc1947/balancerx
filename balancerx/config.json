{
  "server_port": ":8080",
  "health_check_interval": "10s",
  "lb_strategy": "round-robin",
  "backends": [
    {
      "url": "http://localhost:8081",
      "weight": 1,
      "health_path": "/health"
    },
    {
      "url": "http://localhost:8082",
      "weight": 1,
      "health_path": "/"
    }
  ]
}